{
  "name": "kleinpaket-eligibility-checker",
  "version": "1.0.0",
  "description": "A responsive single-page web app for checking Kleinpaket eligibility and calculating shipping costs, VAT, Amazon fees, and profit analysis",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-sqlite3": "^0.9.13",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "decimal.js": "^10.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.14.0",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "kleinpaket",
    "shipping",
    "ecommerce",
    "profit-calculator",
    "europe"
  ],
  "author": "Kleinpaket Eligibility Checker",
  "license": "MIT"
}
